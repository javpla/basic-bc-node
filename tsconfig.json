{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["es2017", "esnext.asynciterable"],
    "outDir": "build",
    "rootDir": "src",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "declaration": false,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": []
}